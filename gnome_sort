def gnomeSort(array):
    x = 0
    while x < len(array)-1:
        if array[x] > array[x+1]:
            temp = array[x+1]
            array[x+1] = array[x]
            array[x] = temp
            if x > 0:
                x -= 1
        elif array[x] <= array[x+1]:
            x += 1
    return array

array = [6, 5, 4, 14, 11, 3]
gnomeSort(array)
for i in array:
    print(i,end=" ")
  
